package com.fs.test;

public class CharTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		char c = 'A';
		char c1 = 'Äã';
		System.out.println("c == " + c);
		System.out.println("c1 == " + c1);
		
		char c2 = '\u0061';
		System.out.println("c2 == " + c2);
		
		char c3 = 39236;
		System.out.println("c3 == " + c3);
	}

}
